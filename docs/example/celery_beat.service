[Unit]
Description=Celery Beat
After=network.target

[Service]
User=user
WorkingDirectory=/home/user/separator
ExecStart=/home/user/separator/.venv/bin/celery -A config.celery_app beat -l INFO --scheduler django_celery_beat.schedulers:DatabaseScheduler
Restart=always

[Install]
WantedBy=multi-user.target
